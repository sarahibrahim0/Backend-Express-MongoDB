{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],

  "functions": {
    "api/test.js": {
      "memory":  1024,
      "maxDuration": 10
    },
    "api/*.js": {
      "memory":  1024,
      "maxDuration": 10
    }
  }
}